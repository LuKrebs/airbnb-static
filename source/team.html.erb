<div class="text-center">
  <h1>Our Team</h1>
  <ul class="list-inline">
  <% data.team.each do |member, info| %>
    <li>
      <h2 class="text-center"><%= info.name %></h2>
      <%= image_tag "#{info.image}", class: "avatar-large-large" %>
    </li>
    <% end %>
  </ul>
</div>
<div class="text-center">
  <h3 class="padded">
    <a href="mailto:contact@airbnb.com">
      <h3><i class="fa fa-envelope"></i> contact@airbnb.com</h3>
    </a>
  </h3>
</div>
